<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.chklist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Veggie"
            android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".Bready" android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".Meaty" android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity android:name=".Fruity" android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".Milky" android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".Finally2" android:configChanges="orientation|screenSize|keyboardHidden"></activity>
    </application>

</manifest>